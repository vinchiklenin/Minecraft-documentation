langs: ['ru']
vcs: true
mtimes: true
authors: true
extensions:
  - name: mdit-plugins
    plugins:
    - '@diplodoc/transform/lib/plugins/checkbox'
  - github-vcs

docs-viewer:
  themes: ['dark']
  langs: ['ru']

  logo-options: # Логотип в левом верхнем углу
    url: https://info.minecraft.vinchiklenin.ru
    src: https://storage.yandexcloud.net/minecraft/MineServerLogo.svg
    src-dark: https://storage.yandexcloud.net/minecraft/MineServerLogo.svg
    img-has:
      height: 30px #тут советую ставить 30 px +-
      offset: true

resources:
  style:
    - _assets/style/custom.css
  csp:
    - "font-src":
        - "https://vinchiklenin.ru"
